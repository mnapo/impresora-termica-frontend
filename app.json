{
  "expo": {
    "name": "ticketeador",
    "slug": "ticketeador",
    "version": "1.0.0",
    "newArchEnabled": true,
    "scheme": "ticketeador",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.ticketeador"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2fdac53d-3033-44fc-b301-a74c35d1bac3"
      }
    },
    "fonts": [
      {
        "asset": "./assets/fonts/Ticketing.ttf",
        "name": "Ticketing"
      }
    ],
    "owner": "mnap"
  }
}
